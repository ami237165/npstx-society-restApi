{
  "name": "societyapi",
  "version": "1.0.0",
  "description": "",
  "main": ".eslintrc.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "cross-env NODE_ENV=production nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ami237165/npstx-society-restApi.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ami237165/npstx-society-restApi/issues"
  },
  "homepage": "https://github.com/ami237165/npstx-society-restApi#readme",
  "dependencies": {
    "@nestjs/common": "^9.0.2",
    "@nestjs/mapped-types": "^1.1.0",
    "@nestjs/mongoose": "^9.2.0",
    "@nestjs/platform-express": "^9.0.3",
    "@nestjs/swagger": "^6.0.3",
    "@nestjs/testing": "^9.0.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "cross-env": "^7.0.3",
    "firebase": "^9.9.0",
    "source-map-support": "^0.5.21",
    "winston": "^3.8.1"
  },
  "devDependencies": {
    "@types/jest": "^28.1.5",
    "typescript": "^4.7.4"
  }
}
